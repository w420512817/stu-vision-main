{
  "name": "student-health-vision",
  "version": "3.0.0",
  "scripts": {
    "dev": "vite",
    "test": "vite --mode test_local",
    "pre": "vite --mode pre_local",
    "build:prod:shandong": "vite build --mode prod_shandong",
    "build:pre:shandong": "vite build --mode pre_shandong",
    "build:test:shandong": "vite build --mode test_shandong",
    "build:prod:chaoju": "vite build --mode prod_chaoju",
    "build:pre:chaoju": "vite build --mode pre_chaoju",
    "build:test:chaoju": "vite build --mode test_chaoju",
    "upload": "node upload.server.js",
    "preview:dist": "vite preview",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,js,jsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,jsx,css,less,vue,html,md}\"",
    "lint:lint-staged": "lint-staged",
    "clean:lib": "pnpm rimraf node_modules pnpm-lock.yaml",
    "prepare": "husky install && cd common-submodule && git config core.hooksPath ../.husky",
    "clear": "pnpm rimraf  dist",
    "publish:test": "npm run clear && npm run build:test && npm run upload",
    "pt": "npm run publish:test",
    "git:push": "sh common-submodule/.shell/push.sh",
    "git:pull": "sh common-submodule/.shell/pull.sh",
    "git:update": "sh common-submodule/.shell/pull.sh && sh common-submodule/.shell/push.sh"
  },
  "dependencies": {
    "@better-scroll/core": "^2.5.0",
    "@better-scroll/mouse-wheel": "^2.5.0",
    "@soybeanjs/vue-admin-layout": "^1.2.1",
    "@soybeanjs/vue-admin-tab": "^1.0.9",
    "@vueuse/core": "^9.12.0",
    "axios": "^1.3.1",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.11.7",
    "echarts": "^5.4.1",
    "jsencrypt": "^3.3.1",
    "lodash-es": "^4.17.21",
    "mockjs": "^1.1.0",
    "naive-ui": "^2.34.3",
    "pinia": "^2.0.30",
    "print-js": "^1.6.0",
    "qrcode": "^1.5.1",
    "qs": "^6.11.0",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "vue-types": "^5.0.2",
    "vue3-sfc-loader": "^0.8.4"
  },
  "devDependencies": {
    "@iconify/json": "^2.1.103",
    "@iconify/vue": "^4.1.0",
    "@unocss/preset-attributify": "^0.49.2",
    "@unocss/preset-uno": "^0.49.2",
    "@unocss/transformer-attributify-jsx": "^0.49.2",
    "@unocss/transformer-directives": "^0.49.2",
    "@unocss/transformer-variant-group": "^0.49.2",
    "@unocss/vite": "^0.49.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "chalk": "^5.2.0",
    "colord": "^2.9.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "lint-staged": "^13.1.0",
    "ora": "~5.0.0",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.2",
    "rollup": "^3.12.1",
    "rollup-plugin-visualizer": "^5.9.0",
    "scp2": "^0.5.0",
    "ssh2": "^1.11.0",
    "unplugin-icons": "^0.15.2",
    "unplugin-vue-components": "^0.23.0",
    "unplugin-vue-define-options": "^1.2.0",
    "vite": "^4.0.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-eslint-parser": "^9.1.0"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=6"
  },
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
